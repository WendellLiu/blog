{
  "name": "wendell-blog",
  "version": "0.0.1",
  "description": "blog created from eleventy-base-blog",
  "scripts": {
    "build": "rm -r _site && ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production postcss ./tailwind.css --o _site/css/style.css",
    "watch": "eleventy --watch",
    "serve": "rm -r _site && eleventy --serve && postcss ./tailwind.css --o _site/css/style.css --watch",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:WendellLiu/blog.git"
  },
  "author": {
    "name": "Wendell Liu",
    "email": "cuk.bas@gmail.com",
    "url": ""
  },
  "license": "MIT",
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "autoprefixer": "^10.3.0",
    "cssnano": "^5.0.6",
    "jsdom": "^16.6.0",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.1.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "sharp": "^0.28.3",
    "tailwindcss": "^2.2.4"
  }
}
